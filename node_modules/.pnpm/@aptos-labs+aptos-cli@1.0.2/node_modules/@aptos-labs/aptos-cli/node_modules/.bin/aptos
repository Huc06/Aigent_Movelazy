#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/huc/working_space/move-agent-chat/node_modules/.pnpm/@aptos-labs+aptos-cli@1.0.2/node_modules/@aptos-labs/aptos-cli/dist/node_modules:/home/huc/working_space/move-agent-chat/node_modules/.pnpm/@aptos-labs+aptos-cli@1.0.2/node_modules/@aptos-labs/aptos-cli/node_modules:/home/huc/working_space/move-agent-chat/node_modules/.pnpm/@aptos-labs+aptos-cli@1.0.2/node_modules/@aptos-labs/node_modules:/home/huc/working_space/move-agent-chat/node_modules/.pnpm/@aptos-labs+aptos-cli@1.0.2/node_modules:/home/huc/working_space/move-agent-chat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/huc/working_space/move-agent-chat/node_modules/.pnpm/@aptos-labs+aptos-cli@1.0.2/node_modules/@aptos-labs/aptos-cli/dist/node_modules:/home/huc/working_space/move-agent-chat/node_modules/.pnpm/@aptos-labs+aptos-cli@1.0.2/node_modules/@aptos-labs/aptos-cli/node_modules:/home/huc/working_space/move-agent-chat/node_modules/.pnpm/@aptos-labs+aptos-cli@1.0.2/node_modules/@aptos-labs/node_modules:/home/huc/working_space/move-agent-chat/node_modules/.pnpm/@aptos-labs+aptos-cli@1.0.2/node_modules:/home/huc/working_space/move-agent-chat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/aptos.js" "$@"
else
  exec node  "$basedir/../../dist/aptos.js" "$@"
fi
